 @using EntityLayer.Concrete
   @model List<AboutService>
<section class="container py-5">
    <div class="row text-center pt-5 pb-3">
        <div class="col-lg-6 m-auto">
            <h1 class="h1">Hizmetlerimiz</h1>
            <p>
            Sizlerin musterilerimizin memnuniyeti assagıda bazı hizmetlerimiz bulunmaktadır.
            </p>
        </div>
    </div>
    <div class="row">
        @foreach (var item in Model)
        {
            
        
        <div class="col-md-6 col-lg-3 pb-5">
            <div class="h-100 py-5 services-icon-wap shadow">
                <div class="h1 text-success text-center"><i class="@item.IconUrl"></i></div>
                <h2 class="h5 mt-4 text-center">@item.Title</h2>
            </div>
        </div>
        }

    </div>
</section>